extends layout

block content
    style.
        p {
            margin: 2px auto;
        }


    div
        h1 Admin
        br

        h2 Job Processes (#{Object.keys(processes).length})
        p global.jobProcesses
        br
        each process in processes
            p Process Number: #{process.processNumber}
            p Endpoint Name: #{process.endpoint}
            br
            // each val2, key2 in process
            //     p #{key2}: #{val2}
            p Job:
            if process.job
                p Number To Use: #{process.job.numberToUse}
                p IP : #{process.job.ip}
                p File Path: #{process.job.uploadedFilePath}
                p Language: #{process.job.language}
                p Model: #{process.job.model}
                p File Size (MB): #{process.job.fileSizeInMB}
                p Translating: #{process.job.shouldTranslate}
                p Upload Duration (s): #{process.job.uploadDurationInSeconds}
                // each val3, key3 in process.job
                //   p #{key3}: #{val3}

            br

        br
        h2 New Queue Items (#{newQueue && newQueue.length})
        p global.newQueue
        br

        each item, index in newQueue
            p Place In Queue: #{index + 1}
            each val, key in item
                p #{key}: #{val}
            br
        br

        br
        h2 Transcription/Download Processes (#{transcriptions && transcriptions.length})
        p global.transcriptions
        br
        each item in transcriptions
          each val, key in item
              p #{key}: #{val}
          br
        br

        h2 Websocket Connections (#{webSocketData && webSocketData.length})
        p global.webSocketData
        br
        each item in webSocketData
            each val, key in item
                p #{key}: #{val}
            br



